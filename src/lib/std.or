let U8_MAX = 255;
let U16_MAX = 65535;
let U32_MAX = 4294967295;
let U64_MAX = 18446744073709551615;

fn max[array] {
    let maximum = 0;

    for i in array {
        if i > maximum {
            maximum = i;
        };
    };

    maximum
};

fn min[array] {
    let minimum = max(array);

    for i in array {
        if i < minimum {
            minimum = i;
        };
    };

    minimum
};

