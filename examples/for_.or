:[int] array = [34, 56, 7, 9, 9, 12, 54, 6, 4, 3, 4, 3, 25, 32];

$say("Array:", array);
$say();

:u8 len = array.len();
:u8 reverse_len = len - 1;

# normal
for item in array do
    $say("Normal Item:", item)
end

; $say();

# reverse.
forx (:u8 idx = reverse_len; idx >= 0; idx -= 1) do
    $say("Reverse Item:", array[idx])
end

; $say();

# skip 2
forx (idx = 0; idx < len; idx += 2) do
    $say("Skip Item:", array[idx])
end

